{
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://yokoai.onrender.com",
    "NEXT_PUBLIC_APP_URL": "https://yoko.vdmnexus.com"
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  },
  "github": {
    "enabled": true,
    "silent": true
  },
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "yokoai-staging.vercel.app"
        }
      ],
      "destination": "https://yokodev.vdmnexus.com",
      "permanent": true
    }
  ]
} 